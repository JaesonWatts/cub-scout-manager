<h1>Create a Den</h1>

<div>
  <%= form_for @den do |f| %>

    <%= render partial: 'layouts/errors', locals: {object: @den} %>

      <br><br>

      <%= f.label :den_name %>
      <%= f.text_field :den_name %>
      <br><br>
      <%= f.label :grade %>
      <%= f.text_field :grade %>
      <br><br>
      <%= f.label :meeting_time %>
      <%= f.text_field :meeting_time %>
      <br><br>  
  


   
    <%= f.submit %>
  <% end %>
</div>
