<div class="container">
  <div class="jumbotron-fluid">
  <h3><%=  link_to "Back to Pack", pack_path %> 

  <h2>Den Name: <%= @den.den_name %></h2> <br><br>
  <h2>Grade Level: <%= @den.grade %></h2><br><br>
  <h2>Meeting Time: <%= @den.meeting_time %></h2><br><br>
  <h2>Scouts:</h2>
  <%= @den.scouts.each do |s| %>

  
    <ul>
      <li><u><%= link_to s.full_name, s%></u></li>
    </ul>
   
  <% end %>
  </div>
</div>